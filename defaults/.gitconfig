[credential]
helper = store

[commit]
gpgsign = false

[gpg]
program = gpg

[pull]
rebase = false

[interactive]
diffFilter = delta --color-only --features=interactive

[merge]
conflictstyle = diff3

[diff]
colorMoved = default

[alias]
count = rev-list --count HEAD
origin = remote add origin
publish = push --set-upstream origin master
rollback = reset --hard HEAD^
undo = reset --soft HEAD^

[http]
version = HTTP/1.1
